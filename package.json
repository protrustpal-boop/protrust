{
  "name": "vite-react-typescript-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "dev": "concurrently \"vite\" \"nodemon --quiet --watch server --ext js,mjs,cjs,json server/index.js\"",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "server": "node index.js",
    "start": "node server/index.js",
    "deploy:manual": "npm run build && netlify deploy --prod --dir=dist",
    "gen:vapid": "node server/scripts/genVapid.js",
    "migrate:brands:dry": "node server/scripts/migrateBrandImagesToCloudinary.js",
    "migrate:brands:apply": "node server/scripts/migrateBrandImagesToCloudinary.js --apply",
    "deploy:netlify": "netlify deploy --build --prod",
    "deploy:netlify:draft": "netlify deploy --build",
    "deploy:netlify:ps": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-netlify.ps1",
    "deploy:netlify:ps:draft": "powershell -ExecutionPolicy Bypass -File ./scripts/deploy-netlify.ps1 -Draft"
  },
  "dependencies": {
    "@hello-pangea/dnd": "^16.5.0",
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@paypal/react-paypal-js": "^8.9.1",
    "@types/dompurify": "^3.0.5",
    "axios": "^1.6.7",
    "bcryptjs": "^2.4.3",
    "cloudinary": "^2.7.0",
    "clsx": "^2.1.1",
    "concurrently": "^8.2.2",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "crypto-random-string": "^5.0.0",
    "date-fns": "^3.6.0",
    "dompurify": "^3.2.7",
    "dotenv": "^16.4.5",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "express": "^4.18.3",
    "express-async-handler": "^1.2.0",
    "google-auth-library": "^9.14.1",
    "http-status-codes": "^2.3.0",
    "i18next": "^23.10.0",
    "i18next-browser-languagedetector": "^7.2.0",
    "i18next-http-backend": "^2.5.0",
    "json2csv": "^5.0.7",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.344.0",
    "mongoose": "^8.2.0",
    "mongoose-paginate-v2": "^1.9.1",
    "multer": "^2.0.2",
    "nanoid": "^5.0.6",
    "nodemailer": "^6.9.11",
    "react": "^18.3.1",
    "react-beautiful-dnd": "^13.1.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^4.1.2",
    "react-fast-marquee": "^1.6.4",
    "react-hot-toast": "^2.6.0",
    "react-i18next": "^14.0.5",
    "react-router-dom": "^6.28.0",
    "recharts": "^2.15.4",
    "tailwind-merge": "^3.3.1",
    "web-push": "^3.6.7",
    "ws": "^8.18.3",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@netlify/edge-functions-bootstrap": "^2.17.1",
    "@types/lodash": "^4.14.202",
    "@types/react": "^18.2.56",
    "@types/react-beautiful-dnd": "^13.1.8",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.18",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "netlify-cli": "^23.8.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.4.35",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  },
  "browserslist": [
    ">0.3%",
    "last 3 versions",
    "Firefox ESR",
    "not dead",
    "not op_mini all"
  ]
}
